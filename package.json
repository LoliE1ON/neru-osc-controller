{
	"name": "neru-osc-controller",
	"productName": "Neru OSC Controller",
	"version": "1.0.1",
	"description": "Neru OSC Controller",
	"main": ".webpack/main",
	"scripts": {
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "cross-env NODE_ENV=production electron-forge make -p win32",
		"lint": "eslint --ext .ts,.tsx ."
	},
	"keywords": [
		"VRChat",
		"OSC"
	],
	"author": "Loli E1ON",
	"contributors": [
		{
			"name": "E1ON <LoliE1ON>",
			"url": "https://github.com/LoliE1ON"
		}
	],
	"license": "MIT",
	"config": {
		"forge": {
			"packagerConfig": {
				"icon": "./icon.ico"
			},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "NeruOSCController"
					}
				}
			],
			"plugins": [
				[
					"@electron-forge/plugin-webpack",
					{
						"mainConfig": "./webpack.main.config.js",
						"renderer": {
							"config": "./webpack.renderer.config.js",
							"entryPoints": [
								{
									"html": "./src/index.html",
									"js": "./src/renderer.ts",
									"name": "main_window"
								}
							]
						}
					}
				]
			]
		}
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.61",
		"@electron-forge/maker-deb": "^6.0.0-beta.61",
		"@electron-forge/maker-rpm": "^6.0.0-beta.61",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.61",
		"@electron-forge/maker-zip": "^6.0.0-beta.61",
		"@electron-forge/plugin-webpack": "6.0.0-beta.61",
		"@trivago/prettier-plugin-sort-imports": "^3.1.1",
		"@types/axios": "^0.14.0",
		"@types/cron": "^2.0.0",
		"@types/node-os-utils": "^1.3.0",
		"@types/on-change": "^1.1.0",
		"@types/puppeteer": "^5.4.7",
		"@types/react": "^18.0.21",
		"@types/react-dom": "^18.0.6",
		"@types/react-router-dom": "^5.3.2",
		"@typescript-eslint/eslint-plugin": "^4.0.1",
		"@typescript-eslint/parser": "^4.0.1",
		"@vercel/webpack-asset-relocator-loader": "1.7.0",
		"css-loader": "^6.0.0",
		"electron": "16.0.5",
		"eslint": "^7.6.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-import": "^2.25.3",
		"fork-ts-checker-webpack-plugin": "^6.0.1",
		"node-loader": "^2.0.0",
		"prettier": "^2.5.1",
		"react-router-dom": "^6.2.1",
		"style-loader": "^3.0.0",
		"ts-loader": "^9.2.2",
		"tsconfig-paths-webpack-plugin": "^3.5.2",
		"typescript": "^4.0.2"
	},
	"dependencies": {
		"@fontsource/roboto": "^4.5.1",
		"@mui/icons-material": "^5.2.5",
		"@mui/lab": "^5.0.0-alpha.65",
		"@mui/material": "^5.2.5",
		"@nanostores/react": "^0.4.0",
		"axios": "^0.24.0",
		"cron": "^2.1.0",
		"cross-env": "^7.0.3",
		"dayjs": "^1.10.7",
		"electron-squirrel-startup": "^1.0.0",
		"electron-unhandled": "^4.0.1",
		"nanostores": "^0.7.0",
		"node-os-utils": "^1.3.7",
		"on-change": "^4.0.1",
		"osc": "^2.4.3",
		"puppeteer": "^19.2.0",
		"rage-edit": "^1.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
